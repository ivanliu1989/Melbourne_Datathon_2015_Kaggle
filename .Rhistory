combine_results <- function(files, header, sep=','){
for(f in 1:length(files)){
#print(f)
if(f == 1){
pred <- read.csv(files[f], header = header, sep = sep)
}else{
pred <- pred + read.csv(files[f], header = header, sep=sep)
}
if(f == length(files)){
pred <- pred/f
}
}
return(pred)
}
